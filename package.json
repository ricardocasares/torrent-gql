{
  "name": "torrent-gql-api",
  "version": "0.0.1",
  "description": "Torrent search GraphQL api",
  "main": "index.js",
  "scripts": {
    "release": "npx semantic-release",
    "alias": "npx now alias --token $NOW_TOKEN",
    "deploy": "npx now --token $NOW_TOKEN",
    "deploy:prod": "npx npm-run-all -s deploy alias",
    "deploy:dev": "npx npm-run-all -s deploy alias:dev",
    "alias:dev": "npx now alias dev-torrent-gql.analogic.al --token $NOW_TOKEN"
  },
  "release": {
    "plugins": [
      "@semantic-release/github",
      "@semantic-release/release-notes-generator"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "polka": "^0.5.1",
    "torrent-search-api": "^2.0.4"
  }
}
